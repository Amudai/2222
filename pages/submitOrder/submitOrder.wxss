/* pages/submitOrder/submitOrder.wxss */
@import "/template/wxss/waterfall.wxss";

/* 整体大容器 */
.container {
  width: 750rpx;
  min-height: 1200rpx;
  padding-bottom: 120rpx;
  background-color: #eee;
  position: relative;
}

.yuyuedaodian {
  position: relative;
  height: 80rpx;
  width: 100%;
  align-items: center;
}

.someaddmsg {
  height: 24rpx;
  font-size: 24rpx;
  font-family: PingFang SC;
  font-weight: 400;
  color: rgba(255, 0, 0, 1);
  line-height: 30rpx;
  margin-top: 16rpx;
  text-align: right;
  margin-bottom: 42rpx;
}

.yuyuedaodian .title {
  width: 119rpx;
  height: 28rpx;
  font-size: 30rpx;
  font-family: PingFang SC;
  font-weight: 500;
  color: rgba(50, 50, 50, 1);
  line-height: 30rpx;
}

.yuyuedaodian .content {
  position: relative;
  height: 100%;
  flex: 1;
  border-bottom: 1rpx solid rgba(0, 0, 0, 0.1);
  font-size: 28rpx;
  font-family: PingFang SC;
  font-weight: 400;
  color: rgba(50, 50, 50, 1);
  line-height: 80rpx;
  text-align: center;
}

.yuyuedaodian image {
  position: absolute;
  right: 0;
  top: 50%;
  transform: translateY(-50%);
  width: 36rpx;
  height: 38rpx;
}

.default_holdspace_font_color {
  color: rgba(147, 144, 144, 1) !important;
  font-size: 28rpx !important;
  font-family: PingFang SC;
  font-weight: 400;
}

.appointment {
  width: 100%;
  height: 102rpx;
  display: flex;
  align-items: center;
  justify-content: flex-end;
  padding-right: 24rpx;
  box-sizing: border-box;
}

.peisong_show {
  display: flex;
  align-items: center;
  width: 702rpx;
  height: 90rpx;
  background: rgba(255, 255, 255, 1);
  border-radius: 20rpx;
  margin-bottom: 20rpx;
  padding-left: 29rpx;
  box-sizing: border-box;
}

.flex_wrapper {
  width: 100%;
  display: flex;
  align-items: center;
  justify-content: space-between;
}

.flex_right {
  /* width: 200rpx; */
  margin-right: 29rpx;
}

.peisong_show text {
  font-size: 36rpx;
  font-family: PingFang SC;
  font-weight: 500;
  color: rgba(50, 50, 50, 1);
}

/* 商品份数 */

.peisong_show .orderNum {
  color: #323232;
  font-size: 30rpx;
  margin-right: 50rpx;
}

/* 商品价格 */

.peisong_show .orderPrice {
  color: #323232;
  font-size: 36rpx;
}

.peisong_show .orderName {
  color: #333;
  font-size: 30rpx;
  /* margin-right: 275rpx; */
}

.xiaolu-wrapper {
  width: 702rpx;
  height: 90rpx;
  background: #fff;
  border-radius: 12rpx;
  margin-bottom: 20rpx;
  padding-left: 29rpx;
  display: flex;
  align-items: center;
  justify-content: space-between;
  box-sizing: border-box;
  color: #333;
  font-size: 30rpx;
}

.xiaolu-num {
  color: #ff1d39;
}

.money_wrapper {
  display: flex;
  justify-content: center;
  align-items: center;
}

.xiaolu-price {
  font-size: 30rpx;
  color: #ff1d39;
  display: flex;
  align-items: center;
}

.xiaolu-no {
  margin-right: 50rpx;
  color: #666;
  font-size: 30rpx;
}

.jiantou {
  width: 48rpx;
  height: 48rpx;
  margin-right: 10rpx;
}

/*用餐方式*/
.eattype_box{
  width: 702rpx;
  background: rgba(255, 255, 255, 1);
  border-radius: 16rpx;
  margin-bottom: 20rpx;
  padding-left: 29rpx;
