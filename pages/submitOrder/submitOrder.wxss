  border-radius: 16rpx;
  margin-bottom: 20rpx;
  padding-left: 29rpx;
  box-sizing: border-box;
}

.canweifei_show text {
  font-size: 36rpx;
  font-family: PingFang SC;
  font-weight: 500;
  color: rgba(50, 50, 50, 1);
}

/* 商品份数 */

.canweifei_show .orderNum {
  color: #323232;
  font-size: 30rpx;
  margin-right: 30rpx;
}

/* 商品价格 */

.canweifei_show .orderPrice {
  color: #323232;
  font-size: 36rpx;
}

.canweifei_show .orderName {
  color: #323232;
  font-size: 30rpx;
  /* margin-right: 155rpx; */
}

/* 店铺详情 */

.storeDetails {
  margin: 20rpx 0;
  width: 702rpx;
  height: 140rpx;
  background-color: #fff;
  border-radius: 16rpx;
}

.orderSinglePrice {
  font-size: 36rpx;
  font-weight: 500;
  color: #333;
}

.detailStorename {
  width: 266rpx;
  height: 25rpx;
  font-size: 26rpx;
  font-family: PingFang SC;
  font-weight: 500;
  color: rgba(50, 50, 50, 1);
  line-height: 30rpx;
}

/* 图标 */
.icon {
  width: 60rpx;
  height: 56rpx;
  margin: 38rpx 26rpx 68rpx 30rpx;
  border-radius: 5px;
}

/* 店名 */
.storeName {
  font-size: 26rpx;
  line-height: 1.6;
  padding: 20rpx 0;
}

/* 具体位置信息 */

.detailLocaltion {
  font-size: 20rpx;
  display: block;
  width: 551rpx;
  height: 77rpx;
  overflow: scroll;
  left: 117rpx;
  top: 76rpx;
  color: #939090;
  line-height: 38rpx;
}

/* 订单详细信息 */

.orderDetails {
  position: relative;
  width: 702rpx;
  /* max-height: 500rpx; */
  border-radius: 16rpx;
  background-color: #fff;
  /* overflow: hidden; */
  margin-bottom: 20rpx;
}

.orderWrap {
  width: 702rpx;
  height: 146rpx;
  display: flex;
  justify-content: space-between;
}

/* 商品图片 */

.orderImg {
  margin: 30rpx 0 0 20rpx;
  width: 100rpx;
  height: 100rpx;
  border-radius: 8rpx;
}

/* 订单描述 */

.orderdesc {
  width: 100%;
  margin-top: 16rpx; 
  display: flex;
  align-items: center;
  justify-content: space-between;
  box-sizing: border-box;
}

.orderDescript {
  font-size: 30rpx;
  color: #333;
}

/* 订单名 */

.orderName {
  top: 106rpx;
  font-size: 22rpx;
  color: #939090;
}

/* 用餐人数 */

.userNum {
  top: 106rpx;
  font-size: 19rpx;
  color: #939090;
}

/* 商品份数 */

.orderNum {
  color: #999;
  font-size: 24rpx;
}

/* 商品价格 */

.orderPrice {
  color: #323232;
  font-size: 36rpx;
}

/* 分割线 */

.boline {
  width: 530rpx;
  display: flex;
  flex-direction: column;
  border-bottom: 1rpx solid rgba(0, 0, 0, 0.1);
  align-items: center;
  justify-content: space-around;
  margin-right: 30rpx;
}

.nobor {
  border-bottom-color: transparent;
}

.num_wrapper {
  width: 100%;
  display: flex;
  flex-direction: row-reverse;
  align-items: center;
  justify-content: space-between;
  box-sizing: border-box;
  margin-bottom: 16rpx;
}

/* 下拉提示 */

.backWhite {
  height: 100rpx;
  width: 100%;
}

.more {
  width: 36rpx;
  height: 20rpx;
}

/* 桌号备注信息 */

.deskRemarks {
  width: 702rpx;
  border-radius: 20rpx;
  background-color: #fff;
  padding: 20rpx 30rpx;
  box-sizing: border-box;
}

/* 桌号 */

.deskNum {
  width: 100%;
  height: 100rpx;
  line-height: 80rpx;
}
  .desc_text {
    font-size: 28rpx;
    line-height: 88rpx;
  width: 60rpx;
}

.remarks {
  /* height: 140rpx; */
  position: relative;
}

.deskNum, .remarks {
  width: 640rpx;
  color: #323232;
  font-size: 30rpx;
}

/*桌号输入框 */

.inputNum, .inputRemarks {
  width: 85%;
  height: 80rpx;
  color: #939090;
  font-size: 28rpx;
  border-bottom: 1px solid #f1f1f1;
  position: static;
  z-index: 0;
}

/* 备注 */

.inputRemarks {
  height: 100rpx;
  border-bottom: 0;
  padding-top: 6rpx;
  width: 80%;
  position: absolute;
  left: 80rpx;
  min-height: 80rpx;
}

.place-holder {
  color: #939090;
  font-size: 28rpx;
}

.clear {
  position: absolute;
  top: 36rpx;
  right: 20rpx;
  width: 31rpx;
  height: 31rpx;
}

/* 底部tarBar */

.tab-bar {
  position: fixed;
  bottom: 0;
  left: 0;
  right: 0;
  height: 110rpx;
  background-color: #fff;
  display: flex;
  padding-left: 20rpx;
  z-index: 10;
}

/* 在线付 */

.moneyBox {
  /* width: 56%; */
  padding: 20rpx 0;
  line-height: 1.4;
}

.goods_money {
  width: 100%;
  padding: 10rpx 30rpx;
  box-sizing: border-box;
  font-size: 30rpx;
  color: #333;
}

.detail-btn {
  margin-right: 260rpx;
}

.onlinePay {
  left: 24rpx;
  top: 25rpx;
  color: #323232;
  font-size: 32rpx;
}

/* 具体金额 */

.totalMoney {
  top: 25rpx;
  left: 127rpx;
  color: #ff1d3a;
  font-size: 32rpx;
}

/* 已优惠 */

.offer, .offerMoney {
  top: 66rpx;
  left: 24rpx;
  color: #939090;
  font-size: 24rpx;
}

.offerMoney {
  color: #fb8f44;
  left: 115rpx;
}

/* 明细 */

.detail {
  width: 60rpx;
  color: #fb8f44;
  font-size: 20rpx;
  line-height: 110rpx;
}

/* 明细图标 */

.detailInco {
  width: 20rpx;
  height: 11rpx;
}

/* 微信支付 */

.settlement {
  position: absolute;
  bottom: 0;
  right: 0rpx;
  background-color: #ff1d3a;
  width: 208rpx;
  height: 116rpx;
  color: #fff;
  line-height: 116rpx;
  font-size: 30rpx;
  text-align: center;
  z-index: 120;
}

/*mask*/

.drawer_screen {
  width: 100%;
  height: 91.8%;
  position: fixed;
  top: 0;
  left: 0;
  z-index: 2000;
  background: #000;
  opacity: 0.2;
  overflow: hidden;
}

.drawer_content {
  padding: 16rpx 24rpx;
}

/*content*/


.moneyDetails {
  height: 80rpx;
  border-bottom: 1rpx solid #f2f2f2;
  font-size: 30rpx;
  color: #737370;
  line-height: 80rpx;
}

.money {
  color: #ff1d3a;
}

.proDetails {
  color: #111;
  font-size: 30rpx;
}

.ainco {
  color: #ff1d3a;
  font-weight: bold;
  font-size: 40rpx;
  margin-right: 16rpx;
}

/* 购物车遮罩层 */

.submit {
  position: fixed;
  left: 0;
  bottom: 0;
}

.mingXi {
  display: inline-block;
  height: 120rpx;
  width: 100rpx;
  float: right;
  line-height: 120rpx;
  margin-right: 20rpx;
}

.sendtag {
  width: 600rpx;
  display: flex;
  align-items: center;
  justify-content: space-between;
  /* display: inline-block;    */
  height: 100rpx;
  /* margin-right: 20rpx;   */
}

.radio {
  margin-left: 30rpx;
}

/*遮罩层 */

.shade {
  width: 100%;
  height: 100%;
  top: 0;
  background: rgba(0, 0, 0, 0.5);
  overflow: hidden;
  z-index: 1;
}

/*显示与隐藏的内容即点击核算后所展示的详细内容 */

.conts {
  width: 100%;
  height: 428rpx;
  background-color: white;
  position: fixed;
  top: 636rpx;
  text-indent: 60rpx;
}

/*显示整个conts */

.show {
  display: block;
}

/*隐藏整个conts */

.hide {
  display: none;
}
