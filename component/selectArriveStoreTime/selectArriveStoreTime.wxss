/* component/selectArriveStoreTime/selectArriveStoreTime.wxss */
.selectArriveTimeContainer{
  height: 100%;
  width: 100%;
  position: fixed;
  background: rgba(0,0,0,0.2);
  z-index: 1;
}
.absolute{
  width:750rpx;
  min-height:724rpx;
  background:rgba(255,255,255,1);
  position: absolute;
  bottom: 0;
  padding: 29rpx 24rpx;
  box-sizing: border-box;
}
.selectArriveTimeContainer .title{
  width:100%;
  height:31rpx;
  font-size:32rpx;
  font-family:PingFang SC;
  font-weight:500;
  color:rgba(50,50,50,1);
  line-height:30rpx;
  margin-bottom: 30rpx;
}
.can_choose_no{
  color:rgba(153,153,153,1) !important;
}
.selectArriveTimeContainer .content{
  display: grid;
  grid-template-rows: 1fr 1fr 1fr 1fr;
  grid-template-columns: 1fr 1fr 1fr 1fr;
  grid-gap: 18rpx;
}
.selectArriveTimeContainer .item{
  width:162rpx;
  height:61rpx;
  background:rgba(229,229,229,1);
  border-radius:5rpx;
  font-size:28rpx;
  font-family:PingFang SC;
  font-weight:500;
  color:rgba(17,17,17,1);
  line-height:61rpx;
  text-align: center;
}
.msg{
  width:100%;
  height:25rpx;
  font-size:26rpx;
  font-family:PingFang SC;
  font-weight:400;
  color:rgba(50,50,50,1);
  line-height:25rpx;
  margin-top: 30rpx;
}
.selectArriveStoreTimeAddClass{
  background:rgba(255,207,209,1) !important;
  color:rgba(253,13,26,1) !important;
}
.leave{
  position: absolute;
  height: 710rpx;
  width: 750rpx;
  top:0;
}