.container {
  display: flex;
  flex-wrap: wrap;
}

.img_wrapper {
  width: 160rpx;
  height: 160rpx;
  border-radius: 8px;
  background: #f5f5f5;
  display: flex;
  align-items: center;
  justify-content: center;
}

.plus {
  width: 64rpx;
  height: 64rpx;
}

.images_wrapper {
  width: 160rpx;
  height: 160rpx;
  border-radius: 8px;
  position: relative;
  margin-right: 40rpx;
}

.images {
  width: 160rpx;
  height: 160rpx;
  border-radius: 8px;
}

.error {
  position: absolute;
  width: 32rpx;
  height: 32rpx;
  top: -20rpx;
  right: -20rpx;
}
